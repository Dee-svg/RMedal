<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--弧形进度条-->
    <declare-styleable name="RoundProgress">
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundWidth" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textSize1" format="dimension" />
        <attr name="max" format="integer" />
        <attr name="textIsDisplayable" format="boolean" />
        <attr name="style">
            <enum name="STROKE" value="0" />
            <enum name="FILL" value="1" />
        </attr>
    </declare-styleable>

    <!--拖拽式层叠卡片-->
    <declare-styleable name="card">
        <attr name="itemMarginTop" format="dimension" />
        <attr name="bottomMarginTop" format="dimension" />
        <attr name="yOffsetStep" format="dimension" />
        <attr name="widthHeightRate" format="float" />
    </declare-styleable>
</resources>