<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="line" >

    <!-- 虚线，宽度为dashWith，空隙的宽度为dashGap，当dashGap=0dp时，为实线 -->
    <stroke
        android:dashGap="3dp"
        android:dashWidth="3dp"
        android:width="2dp"
        android:color="@color/deep_gray_2"/>

</shape>